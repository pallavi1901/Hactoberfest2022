//merging two array 
#include <iostream>
#include<algorithm>
using namespace std;
void merge(int n, int a[], int m, int ar[],int l,int arr[])
{int i=0,j=0,k=0;
while(i<n&&j<m){
if(a[i]<ar[j]){
  arr[k]=a[i] ;
  k++;i++;
}
else{
arr[k]=ar[j];k++;j++;
}

}
//copy first array  element ko
// cout<<k<<" "<<i<<" "<<j<<" "<<endl;
while(i<n){
  arr[k]=ar[i];
  k++;i++;
}
//copy second arry element ko
while(j<m){
    arr[k]=ar[j];k++;j++;
}
}
void print(int ans[],int n){
    
  for(int i=0;i<n;i++) {
   cout<<ans[i]<<" "; 
  } 
}

int main()
{
    int n, m,l;
     cin >> n;
     cin >> m;
    int a[n];
    int ar[m];
    l=n+m;
    int arr[l];
    for (int i = 0; i < n; i++)
        cin >> a[i];
    for (int j = 0; j < m; j++)
        cin >> ar[j];
      sort(a,a+n);
      sort(ar,ar+m);
    merge(n,a,m,ar,l,arr);
    print(arr,l);
    return 0;
}
